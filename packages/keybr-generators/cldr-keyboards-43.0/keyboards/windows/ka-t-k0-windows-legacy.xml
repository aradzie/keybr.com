		<map iso="D05" to="ჱ"/> <!-- T  base=ე -->
		<map iso="D11" to="ჴ"/> <!-- [  base=ხ -->
		<map iso="C01" to="ჶ"/> <!-- A  base=ფ -->
		<map iso="C03" to="ჳ"/> <!-- D  base=ვ -->
		<map iso="B06" to="ჲ"/> <!-- N  base=ი -->
		<map iso="B10" to="ჵ"/> <!-- /  base=ჰ -->
	</keyMap>
	<keyMap modifiers="ctrl+caps?">
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
