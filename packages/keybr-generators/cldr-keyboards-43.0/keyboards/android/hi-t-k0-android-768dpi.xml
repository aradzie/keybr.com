<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="hi-t-k0-android-768dpi">
	<version platform="4.4" number="$Revision$"/>
	<names>
		<name value="Hindi (768dpi)"/>
	</names>
	<keyMap>
		<map iso="D01" to="◌\u{94C}"/> <!-- Q  to= ◌ौ  -->
		<map iso="D02" to="◌\u{948}"/> <!-- W  to= ◌ै  -->
		<map iso="D03" to="◌\u{93E}"/> <!-- E  to= ◌ा  -->
		<map iso="D04" to="◌\u{940}"/> <!-- R  to= ◌ी  -->
		<map iso="D05" to="◌\u{942}"/> <!-- T  to= ◌ू  -->
		<map iso="D06" to="ब" longPress="ब\u{952} %"/> <!-- Y  long=ब॒ %  -->
		<map iso="D07" to="ह"/> <!-- U -->
		<map iso="D08" to="ग" longPress="ज\u{94D}ञ ग\u{93C} ग\u{952} %"/> <!-- I  long=ज्ञ ग़ ग॒ %  -->
		<map iso="D09" to="द"/> <!-- O -->
		<map iso="D10" to="ज" longPress="ज\u{952} ज\u{94D}ञ ज\u{93C} %"/> <!-- P  long=ज॒ ज्ञ ज़ %  -->
		<map iso="D11" to="ड" longPress="ड\u{952} ड\u{93C}"/> <!-- [  long=ड॒ ड़  -->
		<map iso="C01" to="◌\u{94B}"/> <!-- A  to= ◌ो  -->
		<map iso="C02" to="◌\u{947}"/> <!-- S  to= ◌े  -->
		<map iso="C03" to="◌\u{94D}"/> <!-- D  to= ◌्  -->
		<map iso="C04" to="◌\u{93F}"/> <!-- F  to= ◌ि  -->
		<map iso="C05" to="◌\u{941}"/> <!-- G  to= ◌ु  -->
		<map iso="C06" to="प"/> <!-- H -->
		<map iso="C07" to="र" longPress="ऋ र\u{93C} ॠ"/> <!-- J  long=ऋ ऱ ॠ  -->
		<map iso="C08" to="क" longPress="क\u{93C}"/> <!-- K  long=क़  -->
		<map iso="C09" to="त" longPress="त\u{94D}र"/> <!-- L  long=त्र  -->
		<map iso="C10" to="च"/> <!-- ; -->
		<map iso="C11" to="ट"/> <!-- ' -->
		<map iso="B01" to="◌\u{949}"/> <!-- Z  to= ◌ॉ  -->
		<map iso="B02" to="◌\u{902}"/> <!-- X  to= ◌ं  -->
		<map iso="B03" to="म" longPress="ॐ"/> <!-- C -->
		<map iso="B04" to="न" longPress="ञ ङ न\u{93C}"/> <!-- V  long=ञ ङ ऩ  -->
		<map iso="B05" to="व"/> <!-- B -->
		<map iso="B06" to="ल" longPress="ऌ ॡ"/> <!-- N -->
		<map iso="B07" to="स"/> <!-- M -->
		<map iso="B08" to="य" longPress="य़"/> <!-- , -->
		<map iso="B09" to="◌\u{93C}"/> <!-- .  to= ◌़  -->
		<map iso="A02" to=","/> <!-- (key to left of space) -->
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="# ! , ? - : ' @"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt">
		<map iso="D01" to="१" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q  base=◌ौ -->
		<map iso="D02" to="२" longPress="² ⅔"/> <!-- W  base=◌ै -->
		<map iso="D03" to="३" longPress="³ ¾ ⅜"/> <!-- E  base=◌ा -->
		<map iso="D04" to="४" longPress="⁴"/> <!-- R  base=◌ी -->
		<map iso="D05" to="५" longPress="⅝"/> <!-- T  base=◌ू -->
		<map iso="D06" to="६"/> <!-- Y  base=ब -->
		<map iso="D07" to="७" longPress="⅞"/> <!-- U  base=ह -->
		<map iso="D08" to="८"/> <!-- I  base=ग -->
		<map iso="D09" to="९"/> <!-- O  base=द -->
		<map iso="D10" to="०" longPress="ⁿ ∅"/> <!-- P  base=ज -->
		<map iso="C01" to="@"/> <!-- A  base=◌ो -->
		<map iso="C02" to="#"/> <!-- S  base=◌े -->
		<map iso="C03" to="₹" longPress="$ ¢ € £ ¥ ₱"/> <!-- D  base=◌् -->
		<map iso="C04" to="%" longPress="‰"/> <!-- F  base=◌ि -->
		<map iso="C05" to="&amp;"/> <!-- G  base=◌ु -->
		<map iso="C06" to="-" longPress="_ – — ·"/> <!-- H  base=प -->
		<map iso="C07" to="+" longPress="±"/> <!-- J  base=र -->
		<map iso="C08" to="(" longPress="&lt; { ["/> <!-- K  base=क -->
		<map iso="C09" to=")" longPress="&gt; } ]"/> <!-- L  base=त -->
		<map iso="B01" to="*" longPress="† ‡ ★"/> <!-- Z  base=◌ॉ -->
		<map iso="B02" to="\u{22}" longPress="“ ” « »"/> <!-- X  base=◌ं  to= "  -->
		<map iso="B03" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- C  base=म -->
		<map iso="B04" to=":"/> <!-- V  base=न -->
		<map iso="B05" to=";"/> <!-- B  base=व -->
		<map iso="B06" to="!" longPress="¡"/> <!-- N  base=ल -->
		<map iso="B07" to="?" longPress="¿"/> <!-- M  base=स -->
		<map iso="A00" to="_"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="."/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt+shift">
		<map iso="D01" to="~"/> <!-- Q  base=◌ौ -->
		<map iso="D02" to="`"/> <!-- W  base=◌ै -->
		<map iso="D03" to="|"/> <!-- E  base=◌ा -->
		<map iso="D04" to="•" longPress="♪ ♥ ♠ ♦ ♣"/> <!-- R  base=◌ी -->
		<map iso="D05" to="√"/> <!-- T  base=◌ू -->
		<map iso="D06" to="Π" longPress="π"/> <!-- Y  base=ब -->
		<map iso="D07" to="÷"/> <!-- U  base=ह -->
		<map iso="D08" to="×"/> <!-- I  base=ग -->
		<map iso="D09" to="¶" longPress="§"/> <!-- O  base=द -->
		<map iso="D10" to="∆"/> <!-- P  base=ज -->
		<map iso="C01" to="£"/> <!-- A  base=◌ो -->
		<map iso="C02" to="€"/> <!-- S  base=◌े -->
		<map iso="C03" to="$" longPress="¢"/> <!-- D  base=◌् -->
		<map iso="C04" to="¢"/> <!-- F  base=◌ि -->
		<map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G  base=◌ु -->
		<map iso="C06" to="°" longPress="′ ″"/> <!-- H  base=प -->
		<map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J  base=र -->
		<map iso="C08" to="{"/> <!-- K  base=क -->
		<map iso="C09" to="}"/> <!-- L  base=त -->
		<map iso="B01" to="\"/> <!-- Z  base=◌ॉ -->
		<map iso="B02" to="©"/> <!-- X  base=◌ं -->
		<map iso="B03" to="®"/> <!-- C  base=म -->
		<map iso="B04" to="™"/> <!-- V  base=न -->
		<map iso="B05" to="℅"/> <!-- B  base=व -->
		<map iso="B06" to="["/> <!-- N  base=ल -->
		<map iso="B07" to="]"/> <!-- M  base=स -->
		<map iso="A00" to="&lt;" longPress="‹ ≤ «"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="&gt;" longPress="› ≥ »"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="."/> <!-- (key to right of space) -->
	</keyMap>
</keyboard>
