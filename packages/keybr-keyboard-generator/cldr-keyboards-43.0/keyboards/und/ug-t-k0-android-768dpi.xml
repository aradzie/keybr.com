<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<!-- 
	Prepared by Dilmurat Abduqayyum <d.abduqayyum@gmail.com>, reviewed by Waris Abdukerim Janbaz <oyghan@gmail.com> of the Uyghur Computer Science Association http://ukij.org 
-->
<keyboard locale="ug-t-k0-android-768dpi">
	<version platform="4.4" number="$Revision$"/>
	<names>
		<name value="Uyghur (768dpi)"/>
	</names>
	<keyMap>
		<map iso="D01" to="چ"/>
		<map iso="D02" to="ۋ"/>
		<map iso="D03" to="ې" longPress="ئې"/>
		<map iso="D04" to="ر"/>
		<map iso="D05" to="ت"/>
		<map iso="D06" to="ي"/>
		<map iso="D07" to="ۇ" longPress="ئۇ"/>
		<map iso="D08" to="ڭ"/>
		<map iso="D09" to="و" longPress="ئو"/>
		<map iso="D10" to="پ"/>
		<map iso="C01" to="ھ"/>
		<map iso="C02" to="س"/>
		<map iso="C03" to="د" longPress="ژ"/>
		<map iso="C04" to="ا" longPress="ف ئا"/>
		<map iso="C05" to="ە" longPress="گ ئە"/>
		<map iso="C06" to="ى" longPress="خ ئى"/>
		<map iso="C07" to="ق" longPress="ج"/>
		<map iso="C08" to="ك" longPress="ۆ ئۆ"/>
		<map iso="C09" to="ل" longPress="لا"/>
		<map iso="B01" to="ز"/>
		<map iso="B02" to="ش"/>
		<map iso="B03" to="غ"/>
		<map iso="B04" to="ۈ" longPress="ئۈ"/>
		<map iso="B05" to="ب"/>
		<map iso="B06" to="ن"/>
		<map iso="B07" to="م"/>
		<map iso="B08" to="ئ"/>
		<map iso="A02" to="،"/> <!-- (key to left of space) -->
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="\u{22} ' # - : ! ، ؟ @ &amp; % + ؛ / ) ("/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="shift caps">
		<map iso="D01" to="1"/>
		<map iso="D02" to="2"/>
		<map iso="D03" to="3"/>
		<map iso="D04" to="4"/>
		<map iso="D05" to="5"/>
		<map iso="D06" to="6"/>
		<map iso="D07" to="7"/>
		<map iso="D08" to="8"/>
		<map iso="D09" to="9"/>
		<map iso="D10" to="0"/>
		<map iso="C01" to="«"/>
		<map iso="C02" to="»"/>
		<map iso="C03" to="ژ"/>
		<map iso="C04" to="ف"/>
		<map iso="C05" to="گ"/>
		<map iso="C06" to="خ"/>
		<map iso="C07" to="ج"/>
		<map iso="C08" to="ۆ"/>
		<map iso="C09" to="لا"/>
		<map iso="B01" to="*" longPress="★ ٭"/>
		<map iso="B02" to="\u{22}" longPress="“ ” « »"/>
		<map iso="B03" to="(" longPress="﴿ &gt; } ]"/>
		<map iso="B04" to=")" longPress="﴾ &lt; { ["/>
		<map iso="B05" to="؟" longPress="?"/>
		<map iso="B06" to="؛" longPress=";"/>
		<map iso="B07" to=":"/>
		<map iso="A02" to="!"/> <!-- (key to left of space) -->
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="؟ ؛ ! : - / ' \u{22}"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt">
		<map iso="D01" to="1" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q  base=چ -->
		<map iso="D02" to="2" longPress="² ⅔"/> <!-- W  base=ۋ -->
		<map iso="D03" to="3" longPress="³ ¾ ⅜"/> <!-- E  base=ې -->
		<map iso="D04" to="4" longPress="⁴"/> <!-- R  base=ر -->
		<map iso="D05" to="5" longPress="⅝"/> <!-- T base=ت  -->
		<map iso="D06" to="6"/> <!-- Y base=ي  -->
		<map iso="D07" to="7" longPress="⅞"/> <!-- U  base=ۇ -->
		<map iso="D08" to="8"/> <!-- I  base=ڭ -->
		<map iso="D09" to="9"/> <!-- O  base=و -->
		<map iso="D10" to="0" longPress="ⁿ ∅"/> <!-- P  base=پ -->
		<map iso="C01" to="@"/> <!-- A base=ھ  -->
		<map iso="C02" to="#"/> <!-- S  base=س -->
		<map iso="C03" to="$" longPress="¢ £ € ¥ ₱"/> <!-- D  base=د -->
		<map iso="C04" to="%" longPress="‰"/> <!-- F  base=ا -->
		<map iso="C05" to="&amp;"/> <!-- G  base=ە -->
		<map iso="C06" to="-" longPress="_ – — ·"/> <!-- H  base=ى -->
		<map iso="C07" to="+" longPress="±"/> <!-- J  base=ق -->
		<map iso="C08" to="(" longPress="&lt; { ["/> <!-- K  ك -->
		<map iso="C09" to=")" longPress="&gt; } ]"/> <!-- L  ل -->
		<map iso="B01" to="*" longPress="† ‡ ★"/> <!-- Z  base=ز -->
		<map iso="B02" to="\u{22}" longPress="“ ” « »"/> <!-- X base=ش -->
		<map iso="B03" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- C  base=غ -->
		<map iso="B04" to=":"/> <!-- V  base=ۈ -->
		<map iso="B05" to=";"/> <!-- B  base=ب -->
		<map iso="B06" to="!" longPress="¡"/> <!-- N  base=ن -->
		<map iso="B07" to="?" longPress="¿"/> <!-- M  base=م -->
		<map iso="A00" to="_"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt+shift">
		<map iso="D01" to="~"/> <!-- Q  base=چ -->
		<map iso="D02" to="`"/> <!-- W  base=ۋ -->
		<map iso="D03" to="|"/> <!-- E  base=ې -->
		<map iso="D04" to="•" longPress="♪ ♥ ♠ ♦ ♣"/> <!-- R -->
		<map iso="D05" to="√"/> <!-- T  base=ت -->
		<map iso="D06" to="Π" longPress="π"/> <!-- Y -->
		<map iso="D07" to="÷"/> <!-- U  base=ۇ -->
		<map iso="D08" to="×"/> <!-- I  base=ڭ -->
		<map iso="D09" to="¶" longPress="§"/> <!-- O -->
		<map iso="D10" to="∆"/> <!-- P  base=پ -->
		<map iso="C01" to="£"/> <!-- A  base=ھ -->
		<map iso="C02" to="¢"/> <!-- S  base=س -->
		<map iso="C03" to="€"/> <!-- D  base=د -->
		<map iso="C04" to="¥"/> <!-- F  base=ا -->
		<map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G  base=ە -->
		<map iso="C06" to="°" longPress="′ ″"/> <!-- H  base=ى -->
		<map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J  base=ق -->
		<map iso="C08" to="{"/> <!-- K  base=ك -->
		<map iso="C09" to="}"/> <!-- L  base=ل -->
		<map iso="B01" to="\"/> <!-- Z  base=ز -->
		<map iso="B02" to="©"/> <!-- X  base=ش -->
		<map iso="B03" to="®"/> <!-- C  base=غ -->
		<map iso="B04" to="™"/> <!-- V  base=ۈ -->
		<map iso="B05" to="℅"/> <!-- B  base=ب -->
		<map iso="B06" to="["/> <!-- N  base=ن -->
		<map iso="B07" to="]"/> <!-- M  base=م -->
		<map iso="A00" to="&lt;" longPress="‹ ≤ «"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="&gt;" longPress="› ≥ »"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
</keyboard>
