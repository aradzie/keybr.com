		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="# ! , ? - : ' @"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="shift caps">
		<map iso="D01" to="Q"/>
		<map iso="D02" to="W"/>
		<map iso="D03" to="E" longPress="É È Ê Ë Ę"/>
		<map iso="D04" to="R" longPress="Ř"/>
		<map iso="D05" to="T" longPress="Ť Þ"/>
		<map iso="D06" to="Y" longPress="Ý Ÿ Ü"/>
