		<map iso="D09" to="O" longPress="Ó Ò Ô Õ Ō"/>
		<map iso="D10" to="P"/>
		<map iso="D11" to="Å"/> <!-- [ -->
		<map iso="C01" to="A" longPress="Á À Â Ą Ã"/>
		<map iso="C02" to="S" longPress="Ś Š Ş SS"/>
		<map iso="C03" to="D" longPress="Ð Ď"/>
		<map iso="C04" to="F"/>
		<map iso="C05" to="G"/>
		<map iso="C06" to="H"/>
		<map iso="C07" to="J"/>
		<map iso="C08" to="K"/>
