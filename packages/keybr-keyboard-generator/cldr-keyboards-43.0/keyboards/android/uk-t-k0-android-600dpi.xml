<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="uk-t-k0-android-600dpi">
	<version platform="4.4" number="$Revision$"/>
	<names>
		<name value="Ukrainian (600dpi)"/>
	</names>
	<keyMap>
