		<map iso="D06" to="Π" longPress="π"/> <!-- Y  base=н -->
		<map iso="D07" to="÷"/> <!-- U  base=г -->
		<map iso="D08" to="×"/> <!-- I  base=ш -->
		<map iso="D09" to="¶" longPress="§"/> <!-- O  base=щ -->
		<map iso="D10" to="∆"/> <!-- P  base=з -->
		<map iso="C01" to="£"/> <!-- A  base=ф -->
		<map iso="C02" to="€"/> <!-- S  base=і -->
		<map iso="C03" to="$" longPress="¢"/> <!-- D  base=в -->
		<map iso="C04" to="¢"/> <!-- F  base=а -->
		<map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G  base=п -->
		<map iso="C06" to="°" longPress="′ ″"/> <!-- H  base=р -->
		<map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J  base=о -->
		<map iso="C08" to="{"/> <!-- K  base=л -->
		<map iso="C09" to="}"/> <!-- L  base=д -->
		<map iso="B01" to="\"/> <!-- Z  base=я -->
		<map iso="B02" to="©"/> <!-- X  base=ч -->
		<map iso="B03" to="®"/> <!-- C  base=с -->
		<map iso="B04" to="™"/> <!-- V  base=м -->
