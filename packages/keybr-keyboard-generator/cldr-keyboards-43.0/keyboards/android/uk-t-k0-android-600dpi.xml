		<map iso="D09" to="9"/> <!-- O  base=щ -->
		<map iso="D10" to="0" longPress="ⁿ ∅"/> <!-- P  base=з -->
		<map iso="C01" to="@"/> <!-- A  base=ф -->
		<map iso="C02" to="#"/> <!-- S  base=і -->
		<map iso="C03" to="₴" longPress="$ ¢ € £ ¥ ₱"/> <!-- D  base=в -->
