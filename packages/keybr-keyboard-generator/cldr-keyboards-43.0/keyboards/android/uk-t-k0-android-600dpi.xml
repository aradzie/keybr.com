		<map iso="C07" to="+" longPress="±"/> <!-- J  base=о -->
		<map iso="C08" to="(" longPress="&lt; { ["/> <!-- K  base=л -->
		<map iso="C09" to=")" longPress="&gt; } ]"/> <!-- L  base=д -->
		<map iso="B01" to="\"/> <!-- Z  base=я -->
		<map iso="B02" to="="/> <!-- X  base=ч -->
		<map iso="B03" to="*" longPress="† ‡ ★"/> <!-- C  base=с -->
		<map iso="B04" to="\u{22}" longPress="“ ” « »"/> <!-- V  base=м  to= "  -->
		<map iso="B05" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- B  base=и -->
		<map iso="B06" to=":"/> <!-- N  base=т -->
		<map iso="B07" to=";"/> <!-- M  base=ь -->
		<map iso="B08" to="!" longPress="¡"/> <!-- ,  base=б -->
