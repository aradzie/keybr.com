	<keyMap modifiers="opt">
		<map iso="D01" to="1" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q  base=й -->
		<map iso="D02" to="2" longPress="² ⅔"/> <!-- W  base=ц -->
		<map iso="D03" to="3" longPress="³ ¾ ⅜"/> <!-- E  base=у -->
