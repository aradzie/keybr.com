<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="hr-t-k0-android-600dpi">
	<version platform="4.4" number="$Revision$"/>
	<names>
		<name value="Croatian (600dpi)"/>
	</names>
	<keyMap>
		<map iso="D01" to="q"/>
		<map iso="D02" to="w"/>
		<map iso="D03" to="e"/>
		<map iso="D04" to="r"/>
		<map iso="D05" to="t"/>
		<map iso="D06" to="z" longPress="ž ź ż"/> <!-- Y -->
		<map iso="D07" to="u"/>
		<map iso="D08" to="i"/>
		<map iso="D09" to="o"/>
		<map iso="D10" to="p"/>
		<map iso="C01" to="a"/>
		<map iso="C02" to="s" longPress="š ś ß"/>
		<map iso="C03" to="d" longPress="đ"/>
		<map iso="C04" to="f"/>
		<map iso="C05" to="g"/>
		<map iso="C06" to="h"/>
		<map iso="C07" to="j"/>
		<map iso="C08" to="k"/>
		<map iso="C09" to="l"/>
		<map iso="B01" to="y"/> <!-- Z -->
		<map iso="B02" to="x"/>
		<map iso="B03" to="c" longPress="č ć ç"/>
		<map iso="B04" to="v"/>
		<map iso="B05" to="b"/>
		<map iso="B06" to="n" longPress="ñ ń"/>
		<map iso="B07" to="m"/>
		<map iso="B08" to="!"/> <!-- , -->
		<map iso="B09" to="?"/> <!-- . -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space) -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="shift caps">
		<map iso="D01" to="Q"/>
		<map iso="D02" to="W"/>
		<map iso="D03" to="E"/>
		<map iso="D04" to="R"/>
		<map iso="D05" to="T"/>
		<map iso="D06" to="Z" longPress="Ž Ź Ż"/> <!-- Y -->
		<map iso="D07" to="U"/>
		<map iso="D08" to="I"/>
		<map iso="D09" to="O"/>
		<map iso="D10" to="P"/>
		<map iso="C01" to="A"/>
		<map iso="C02" to="S" longPress="Š Ś SS"/>
		<map iso="C03" to="D" longPress="Đ"/>
		<map iso="C04" to="F"/>
		<map iso="C05" to="G"/>
		<map iso="C06" to="H"/>
		<map iso="C07" to="J"/>
		<map iso="C08" to="K"/>
		<map iso="C09" to="L"/>
		<map iso="B01" to="Y"/> <!-- Z -->
		<map iso="B02" to="X"/>
		<map iso="B03" to="C" longPress="Č Ć Ç"/>
		<map iso="B04" to="V"/>
		<map iso="B05" to="B"/>
		<map iso="B06" to="N" longPress="Ñ Ń"/>
		<map iso="B07" to="M"/>
		<map iso="B08" to="!"/> <!-- , -->
		<map iso="B09" to="?"/> <!-- . -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space) -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt">
		<map iso="D01" to="1" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q -->
		<map iso="D02" to="2" longPress="² ⅔"/> <!-- W -->
		<map iso="D03" to="3" longPress="³ ¾ ⅜"/> <!-- E -->
		<map iso="D04" to="4" longPress="⁴"/> <!-- R -->
		<map iso="D05" to="5" longPress="⅝"/> <!-- T -->
		<map iso="D06" to="6"/> <!-- Y  base=z -->
		<map iso="D07" to="7" longPress="⅞"/> <!-- U -->
		<map iso="D08" to="8"/> <!-- I -->
		<map iso="D09" to="9"/> <!-- O -->
		<map iso="D10" to="0" longPress="ⁿ ∅"/> <!-- P -->
		<map iso="C01" to="@"/> <!-- A -->
		<map iso="C02" to="#"/> <!-- S -->
		<map iso="C03" to="$" longPress="¢ £ € ¥ ₱"/> <!-- D -->
		<map iso="C04" to="%" longPress="‰"/> <!-- F -->
		<map iso="C05" to="&amp;"/> <!-- G -->
		<map iso="C06" to="-" longPress="_ – — ·"/> <!-- H -->
		<map iso="C07" to="+" longPress="±"/> <!-- J -->
		<map iso="C08" to="(" longPress="&lt; { ["/> <!-- K -->
		<map iso="C09" to=")" longPress="&gt; } ]"/> <!-- L -->
		<map iso="B01" to="\"/> <!-- Z  base=y -->
		<map iso="B02" to="="/> <!-- X -->
		<map iso="B03" to="*" longPress="† ‡ ★"/> <!-- C -->
		<map iso="B04" to="\u{22}" longPress="“ ” « »"/> <!-- V  to= "  -->
		<map iso="B05" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- B -->
		<map iso="B06" to=":"/> <!-- N -->
		<map iso="B07" to=";"/> <!-- M -->
		<map iso="B08" to="!" longPress="¡"/> <!-- , -->
		<map iso="B09" to="?" longPress="¿"/> <!-- . -->
		<map iso="A00" to="_"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space) -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt+shift">
		<map iso="D01" to="~"/> <!-- Q -->
		<map iso="D02" to="`"/> <!-- W -->
		<map iso="D03" to="|"/> <!-- E -->
		<map iso="D04" to="•" longPress="♪ ♥ ♠ ♦ ♣"/> <!-- R -->
		<map iso="D05" to="√"/> <!-- T -->
		<map iso="D06" to="Π" longPress="π"/> <!-- Y  base=z -->
		<map iso="D07" to="÷"/> <!-- U -->
		<map iso="D08" to="×"/> <!-- I -->
		<map iso="D09" to="¶" longPress="§"/> <!-- O -->
		<map iso="D10" to="∆"/> <!-- P -->
		<map iso="C01" to="£"/> <!-- A -->
		<map iso="C02" to="¢"/> <!-- S -->
		<map iso="C03" to="€"/> <!-- D -->
		<map iso="C04" to="¥"/> <!-- F -->
		<map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G -->
		<map iso="C06" to="°" longPress="′ ″"/> <!-- H -->
		<map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J -->
		<map iso="C08" to="{"/> <!-- K -->
		<map iso="C09" to="}"/> <!-- L -->
		<map iso="B01" to="\"/> <!-- Z  base=y -->
		<map iso="B02" to="©"/> <!-- X -->
		<map iso="B03" to="®"/> <!-- C -->
		<map iso="B04" to="™"/> <!-- V -->
		<map iso="B05" to="℅"/> <!-- B -->
		<map iso="B06" to="["/> <!-- N -->
		<map iso="B07" to="]"/> <!-- M -->
		<map iso="B08" to="¡"/> <!-- ,  base=! -->
		<map iso="B09" to="¿"/> <!-- .  base=? -->
		<map iso="A00" to="&lt;" longPress="‹ ≤ «"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="&gt;" longPress="› ≥ »"/> <!-- (2 keys to left of space)  base=/ -->
		<map iso="A02" to=" "/> <!-- (key to left of space) -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
</keyboard>
