<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="mn-t-k0-android">
	<version platform="4.4" number="$Revision$"/>
	<names>
		<name value="Mongolian"/>
	</names>
	<keyMap>
		<map iso="D01" to="ф"/> <!-- Q -->
		<map iso="D02" to="ц"/> <!-- W -->
		<map iso="D03" to="у"/> <!-- E -->
		<map iso="D04" to="ж"/> <!-- R -->
		<map iso="D05" to="э"/> <!-- T -->
		<map iso="D06" to="н"/> <!-- Y -->
		<map iso="D07" to="г"/> <!-- U -->
		<map iso="D08" to="ш" longPress="щ"/> <!-- I -->
		<map iso="D09" to="ү"/> <!-- O -->
		<map iso="D10" to="з"/> <!-- P -->
		<map iso="D11" to="к"/> <!-- [ -->
		<map iso="C01" to="й"/> <!-- A -->
		<map iso="C02" to="ы"/> <!-- S -->
		<map iso="C03" to="б"/> <!-- D -->
		<map iso="C04" to="ө"/> <!-- F -->
		<map iso="C05" to="а"/> <!-- G -->
		<map iso="C06" to="х"/> <!-- H -->
		<map iso="C07" to="р"/> <!-- J -->
		<map iso="C08" to="о"/> <!-- K -->
		<map iso="C09" to="л"/> <!-- L -->
		<map iso="C10" to="д"/> <!-- ; -->
		<map iso="C11" to="п"/> <!-- ' -->
		<map iso="B01" to="я"/> <!-- Z -->
		<map iso="B02" to="ч"/> <!-- X -->
		<map iso="B03" to="ё" longPress="е"/> <!-- C -->
		<map iso="B04" to="с"/> <!-- V -->
		<map iso="B05" to="м"/> <!-- B -->
		<map iso="B06" to="и"/> <!-- N -->
		<map iso="B07" to="т"/> <!-- M -->
		<map iso="B08" to="ь" longPress="ъ"/> <!-- , -->
		<map iso="B09" to="в" longPress="ю"/> <!-- . -->
		<map iso="A02" to=","/> <!-- (key to left of space) -->
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="# ! , ? - : ' @"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="shift caps">
		<map iso="D01" to="Ф"/> <!-- Q -->
		<map iso="D02" to="Ц"/> <!-- W -->
		<map iso="D03" to="У"/> <!-- E -->
		<map iso="D04" to="Ж"/> <!-- R -->
		<map iso="D05" to="Э"/> <!-- T -->
		<map iso="D06" to="Н"/> <!-- Y -->
		<map iso="D07" to="Г"/> <!-- U -->
		<map iso="D08" to="Ш" longPress="Щ"/> <!-- I -->
		<map iso="D09" to="Ү"/> <!-- O -->
		<map iso="D10" to="З"/> <!-- P -->
		<map iso="D11" to="К"/> <!-- [ -->
		<map iso="C01" to="Й"/> <!-- A -->
		<map iso="C02" to="Ы"/> <!-- S -->
		<map iso="C03" to="Б"/> <!-- D -->
		<map iso="C04" to="Ө"/> <!-- F -->
		<map iso="C05" to="А"/> <!-- G -->
		<map iso="C06" to="Х"/> <!-- H -->
		<map iso="C07" to="Р"/> <!-- J -->
		<map iso="C08" to="О"/> <!-- K -->
		<map iso="C09" to="Л"/> <!-- L -->
		<map iso="C10" to="Д"/> <!-- ; -->
		<map iso="C11" to="П"/> <!-- ' -->
		<map iso="B01" to="Я"/> <!-- Z -->
		<map iso="B02" to="Ч"/> <!-- X -->
		<map iso="B03" to="Ё" longPress="Е"/> <!-- C -->
		<map iso="B04" to="С"/> <!-- V -->
		<map iso="B05" to="М"/> <!-- B -->
		<map iso="B06" to="И"/> <!-- N -->
		<map iso="B07" to="Т"/> <!-- M -->
		<map iso="B08" to="Ь" longPress="Ъ"/> <!-- , -->
		<map iso="B09" to="В" longPress="Ю"/> <!-- . -->
		<map iso="A02" to=","/> <!-- (key to left of space) -->
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="# ! , ? - : ' @"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt">
		<map iso="D01" to="1" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q  base=ф -->
		<map iso="D02" to="2" longPress="² ⅔"/> <!-- W  base=ц -->
		<map iso="D03" to="3" longPress="³ ¾ ⅜"/> <!-- E  base=у -->
		<map iso="D04" to="4" longPress="⁴"/> <!-- R  base=ж -->
		<map iso="D05" to="5" longPress="⅝"/> <!-- T  base=э -->
		<map iso="D06" to="6"/> <!-- Y  base=н -->
		<map iso="D07" to="7" longPress="⅞"/> <!-- U  base=г -->
		<map iso="D08" to="8"/> <!-- I  base=ш -->
		<map iso="D09" to="9"/> <!-- O  base=ү -->
		<map iso="D10" to="0" longPress="ⁿ ∅"/> <!-- P  base=з -->
		<map iso="C01" to="@"/> <!-- A  base=й -->
		<map iso="C02" to="#"/> <!-- S  base=ы -->
		<map iso="C03" to="₮" longPress="$ ¢ € £ ¥ ₱"/> <!-- D  base=б -->
		<map iso="C04" to="%" longPress="‰"/> <!-- F  base=ө -->
		<map iso="C05" to="&amp;"/> <!-- G  base=а -->
		<map iso="C06" to="-" longPress="_ – — ·"/> <!-- H  base=х -->
		<map iso="C07" to="+" longPress="±"/> <!-- J  base=р -->
		<map iso="C08" to="(" longPress="&lt; { ["/> <!-- K  base=о -->
		<map iso="C09" to=")" longPress="&gt; } ]"/> <!-- L  base=л -->
		<map iso="B01" to="*" longPress="† ‡ ★"/> <!-- Z  base=я -->
		<map iso="B02" to="\u{22}" longPress="“ ” « »"/> <!-- X  base=ч  to= "  -->
		<map iso="B03" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- C  base=ё -->
		<map iso="B04" to=":"/> <!-- V  base=с -->
		<map iso="B05" to=";"/> <!-- B  base=м -->
		<map iso="B06" to="!" longPress="¡"/> <!-- N  base=и -->
		<map iso="B07" to="?" longPress="¿"/> <!-- M  base=т -->
		<map iso="A00" to="_"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt+shift">
		<map iso="D01" to="~"/> <!-- Q  base=ф -->
		<map iso="D02" to="`"/> <!-- W  base=ц -->
		<map iso="D03" to="|"/> <!-- E  base=у -->
		<map iso="D04" to="•" longPress="♪ ♥ ♠ ♦ ♣"/> <!-- R  base=ж -->
		<map iso="D05" to="√"/> <!-- T  base=э -->
		<map iso="D06" to="Π" longPress="π"/> <!-- Y  base=н -->
		<map iso="D07" to="÷"/> <!-- U  base=г -->
		<map iso="D08" to="×"/> <!-- I  base=ш -->
		<map iso="D09" to="¶" longPress="§"/> <!-- O  base=ү -->
		<map iso="D10" to="∆"/> <!-- P  base=з -->
		<map iso="C01" to="£"/> <!-- A  base=й -->
		<map iso="C02" to="€"/> <!-- S  base=ы -->
		<map iso="C03" to="$" longPress="¢"/> <!-- D  base=б -->
		<map iso="C04" to="¢"/> <!-- F  base=ө -->
		<map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G  base=а -->
		<map iso="C06" to="°" longPress="′ ″"/> <!-- H  base=х -->
		<map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J  base=р -->
		<map iso="C08" to="{"/> <!-- K  base=о -->
		<map iso="C09" to="}"/> <!-- L  base=л -->
		<map iso="B01" to="\"/> <!-- Z  base=я -->
		<map iso="B02" to="©"/> <!-- X  base=ч -->
		<map iso="B03" to="®"/> <!-- C  base=ё -->
		<map iso="B04" to="™"/> <!-- V  base=с -->
		<map iso="B05" to="℅"/> <!-- B  base=м -->
		<map iso="B06" to="["/> <!-- N  base=и -->
		<map iso="B07" to="]"/> <!-- M  base=т -->
		<map iso="A00" to="&lt;" longPress="‹ ≤ «"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="&gt;" longPress="› ≥ »"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
</keyboard>
