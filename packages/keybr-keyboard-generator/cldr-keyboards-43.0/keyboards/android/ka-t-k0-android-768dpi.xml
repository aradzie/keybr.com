<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="ka-t-k0-android-768dpi">
	<version platform="4.4" number="$Revision$"/>
	<names>
		<name value="Georgian (768dpi)"/>
	</names>
	<keyMap>
		<map iso="D01" to="ქ"/> <!-- Q -->
		<map iso="D02" to="წ"/> <!-- W -->
		<map iso="D03" to="ე" longPress="ჱ"/> <!-- E -->
		<map iso="D04" to="რ"/> <!-- R -->
		<map iso="D05" to="ტ"/> <!-- T -->
		<map iso="D06" to="ყ" longPress="ჸ"/> <!-- Y -->
		<map iso="D07" to="უ"/> <!-- U -->
		<map iso="D08" to="ი" longPress="ჲ"/> <!-- I -->
		<map iso="D09" to="ო"/> <!-- O -->
		<map iso="D10" to="პ"/> <!-- P -->
		<map iso="C01" to="ა" longPress="ჺ"/> <!-- A -->
		<map iso="C02" to="ს"/> <!-- S -->
		<map iso="C03" to="დ"/> <!-- D -->
		<map iso="C04" to="ფ" longPress="ჶ"/> <!-- F -->
		<map iso="C05" to="გ" longPress="ჹ"/> <!-- G -->
		<map iso="C06" to="ჰ" longPress="ჵ"/> <!-- H -->
		<map iso="C07" to="ჯ" longPress="ჷ"/> <!-- J -->
		<map iso="C08" to="კ"/> <!-- K -->
		<map iso="C09" to="ლ"/> <!-- L -->
		<map iso="B01" to="ზ"/> <!-- Z -->
		<map iso="B02" to="ხ" longPress="ჴ"/> <!-- X -->
		<map iso="B03" to="ც"/> <!-- C -->
		<map iso="B04" to="ვ" longPress="ჳ"/> <!-- V -->
		<map iso="B05" to="ბ"/> <!-- B -->
		<map iso="B06" to="ნ" longPress="ჼ"/> <!-- N -->
		<map iso="B07" to="მ"/> <!-- M -->
		<map iso="A02" to=","/> <!-- (key to left of space) -->
		<map iso="A03" to=" "/> <!-- space -->
		<map iso="A04" to="." longPress="# ! , ? - : ' @"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt">
		<map iso="D01" to="1" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q  base=ქ -->
		<map iso="D02" to="2" longPress="² ⅔"/> <!-- W  base=წ -->
		<map iso="D03" to="3" longPress="³ ¾ ⅜"/> <!-- E  base=ე -->
		<map iso="D04" to="4" longPress="⁴"/> <!-- R  base=რ -->
		<map iso="D05" to="5" longPress="⅝"/> <!-- T  base=ტ -->
		<map iso="D06" to="6"/> <!-- Y  base=ყ -->
		<map iso="D07" to="7" longPress="⅞"/> <!-- U  base=უ -->
		<map iso="D08" to="8"/> <!-- I  base=ი -->
		<map iso="D09" to="9"/> <!-- O  base=ო -->
		<map iso="D10" to="0" longPress="ⁿ ∅"/> <!-- P  base=პ -->
		<map iso="C01" to="@"/> <!-- A  base=ა -->
		<map iso="C02" to="#"/> <!-- S  base=ს -->
		<map iso="C03" to="$" longPress="¢ £ € ¥ ₱"/> <!-- D  base=დ -->
		<map iso="C04" to="%" longPress="‰"/> <!-- F  base=ფ -->
		<map iso="C05" to="&amp;"/> <!-- G  base=გ -->
		<map iso="C06" to="-" longPress="_ – — ·"/> <!-- H  base=ჰ -->
		<map iso="C07" to="+" longPress="±"/> <!-- J  base=ჯ -->
		<map iso="C08" to="(" longPress="&lt; { ["/> <!-- K  base=კ -->
		<map iso="C09" to=")" longPress="&gt; } ]"/> <!-- L  base=ლ -->
		<map iso="B01" to="*" longPress="† ‡ ★"/> <!-- Z  base=ზ -->
		<map iso="B02" to="\u{22}" longPress="“ ” « »"/> <!-- X  base=ხ  to= "  -->
		<map iso="B03" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- C  base=ც -->
		<map iso="B04" to=":"/> <!-- V  base=ვ -->
		<map iso="B05" to=";"/> <!-- B  base=ბ -->
		<map iso="B06" to="!" longPress="¡"/> <!-- N  base=ნ -->
		<map iso="B07" to="?" longPress="¿"/> <!-- M  base=მ -->
		<map iso="A00" to="_"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="."/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="opt+shift">
		<map iso="D01" to="~"/> <!-- Q  base=ქ -->
		<map iso="D02" to="`"/> <!-- W  base=წ -->
		<map iso="D03" to="|"/> <!-- E  base=ე -->
		<map iso="D04" to="•" longPress="♪ ♥ ♠ ♦ ♣"/> <!-- R  base=რ -->
		<map iso="D05" to="√"/> <!-- T  base=ტ -->
		<map iso="D06" to="Π" longPress="π"/> <!-- Y  base=ყ -->
		<map iso="D07" to="÷"/> <!-- U  base=უ -->
		<map iso="D08" to="×"/> <!-- I  base=ი -->
		<map iso="D09" to="¶" longPress="§"/> <!-- O  base=ო -->
		<map iso="D10" to="∆"/> <!-- P  base=პ -->
		<map iso="C01" to="£"/> <!-- A  base=ა -->
		<map iso="C02" to="¢"/> <!-- S  base=ს -->
		<map iso="C03" to="€"/> <!-- D  base=დ -->
		<map iso="C04" to="¥"/> <!-- F  base=ფ -->
		<map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G  base=გ -->
		<map iso="C06" to="°" longPress="′ ″"/> <!-- H  base=ჰ -->
		<map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J  base=ჯ -->
		<map iso="C08" to="{"/> <!-- K  base=კ -->
		<map iso="C09" to="}"/> <!-- L  base=ლ -->
		<map iso="B01" to="\"/> <!-- Z  base=ზ -->
		<map iso="B02" to="©"/> <!-- X  base=ხ -->
		<map iso="B03" to="®"/> <!-- C  base=ც -->
		<map iso="B04" to="™"/> <!-- V  base=ვ -->
		<map iso="B05" to="℅"/> <!-- B  base=ბ -->
		<map iso="B06" to="["/> <!-- N  base=ნ -->
		<map iso="B07" to="]"/> <!-- M  base=მ -->
		<map iso="A00" to="&lt;" longPress="‹ ≤ «"/> <!-- (3 keys to left of space) -->
		<map iso="A01" to="&gt;" longPress="› ≥ »"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="."/> <!-- (key to right of space) -->
	</keyMap>
</keyboard>
