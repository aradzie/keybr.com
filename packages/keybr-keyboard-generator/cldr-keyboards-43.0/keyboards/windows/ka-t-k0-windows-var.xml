<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="ka-t-k0-windows-var">
	<version platform="10" number="$Revision$"/>
	<names>
		<name value="Georgian (Ergonomic)"/>
	</names>
	<settings fallback="omit"/>
	<keyMap>
		<map iso="E00" to="„"/> <!-- ` -->
		<map iso="E01" to="!"/> <!-- 1 -->
		<map iso="E02" to="№"/> <!-- 2 -->
		<map iso="E03" to=","/> <!-- 3 -->
		<map iso="E04" to=";"/> <!-- 4 -->
		<map iso="E05" to="%"/> <!-- 5 -->
		<map iso="E06" to=":"/> <!-- 6 -->
		<map iso="E07" to="?"/> <!-- 7 -->
		<map iso="E08" to="."/> <!-- 8 -->
		<map iso="E09" to="("/> <!-- 9 -->
		<map iso="E10" to=")"/> <!-- 0 -->
		<map iso="E11" to="-"/>
		<map iso="E12" to="“"/> <!-- = -->
		<map iso="D01" to="ჩ"/> <!-- Q -->
		<map iso="D02" to="პ"/> <!-- W -->
		<map iso="D03" to="უ"/> <!-- E -->
		<map iso="D04" to="ძ"/> <!-- R -->
		<map iso="D05" to="ჭ"/> <!-- T -->
		<map iso="D06" to="ტ"/> <!-- Y -->
		<map iso="D07" to="თ"/> <!-- U -->
		<map iso="D08" to="ნ"/> <!-- I -->
		<map iso="D09" to="ვ"/> <!-- O -->
		<map iso="D10" to="შ"/> <!-- P -->
		<map iso="D11" to="კ"/> <!-- [ -->
		<map iso="D12" to="ქ"/> <!-- ] -->
		<map iso="C01" to="ხ"/> <!-- A -->
		<map iso="C02" to="ი"/> <!-- S -->
		<map iso="C03" to="ა"/> <!-- D -->
		<map iso="C04" to="ე"/> <!-- F -->
		<map iso="C05" to="ო"/> <!-- G -->
		<map iso="C06" to="დ"/> <!-- H -->
		<map iso="C07" to="მ"/> <!-- J -->
		<map iso="C08" to="ს"/> <!-- K -->
		<map iso="C09" to="რ"/> <!-- L -->
		<map iso="C10" to="ბ"/> <!-- ; -->
		<map iso="C11" to="გ"/> <!-- ' -->
		<map iso="C12" to="/"/> <!-- (key to right of ') -->
		<map iso="B00" to="\"/> <!-- (key to left of Z) -->
		<map iso="B01" to="ჯ"/> <!-- Z -->
		<map iso="B02" to="ჰ"/> <!-- X -->
		<map iso="B03" to="ყ"/> <!-- C -->
		<map iso="B04" to="ღ"/> <!-- V -->
		<map iso="B05" to="ჟ"/> <!-- B -->
		<map iso="B06" to="ზ"/> <!-- N -->
		<map iso="B07" to="ც"/> <!-- M -->
		<map iso="B08" to="ლ"/> <!-- , -->
		<map iso="B09" to="ფ"/> <!-- . -->
		<map iso="B10" to="წ"/> <!-- / -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="shift+caps?">
		<map iso="E00" to="&apos;"/> <!-- `  base=„ -->
		<map iso="E01" to="1"/> <!--   base=! -->
		<map iso="E02" to="2"/> <!--   base=№ -->
		<map iso="E03" to="3"/> <!--   base=, -->
		<map iso="E04" to="4"/> <!--   base=; -->
		<map iso="E05" to="5"/> <!--   base=% -->
		<map iso="E06" to="6"/> <!--   base=: -->
		<map iso="E07" to="7"/> <!--   base=? -->
		<map iso="E08" to="8"/> <!--   base=. -->
		<map iso="E09" to="9"/> <!--   base=( -->
		<map iso="E10" to="0"/> <!--   base=) -->
		<map iso="E11" to="+"/> <!-- - -->
		<map iso="E12" to="="/> <!--   base=“ -->
		<map iso="C12" to="§"/> <!-- (key to right of ')  base=/ -->
		<map iso="B00" to="/"/> <!-- (key to left of Z)  base=\ -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="altR+caps? ctrl+alt+caps?">
		<map iso="E00" to="`"/> <!--   base=„ -->
		<map iso="E01" to="!"/> <!-- 1 -->
		<map iso="E02" to="@"/> <!-- 2  base=№ -->
		<map iso="E03" to="#"/> <!-- 3  base=, -->
		<map iso="E04" to="$"/> <!-- 4  base=; -->
		<map iso="E05" to="€"/> <!-- 5  base=% -->
		<map iso="E06" to="^"/> <!-- 6  base=: -->
		<map iso="E07" to="&amp;"/> <!-- 7  base=? -->
		<map iso="E08" to="*"/> <!-- 8  base=. -->
		<map iso="E09" to="("/> <!-- 9 -->
		<map iso="E10" to=")"/> <!-- 0 -->
		<map iso="E11" to="—"/> <!-- - -->
		<map iso="E12" to="="/> <!--   base=“ -->
		<map iso="D08" to="ჼ"/> <!-- I  base=ნ -->
		<map iso="D09" to="ჳ"/> <!-- O  base=ვ -->
		<map iso="D11" to="["/> <!--   base=კ -->
		<map iso="D12" to="]"/> <!--   base=ქ -->
		<map iso="C01" to="ჴ"/> <!-- A  base=ხ -->
		<map iso="C02" to="ჲ"/> <!-- S  base=ი -->
		<map iso="C03" to="ჺ"/> <!-- D  base=ა -->
		<map iso="C04" to="ჱ"/> <!-- F  base=ე -->
		<map iso="C11" to="ჹ"/> <!-- '  base=გ -->
		<map iso="C12" to="\"/> <!-- (key to right of ')  base=/ -->
		<map iso="B02" to="ჵ"/> <!-- X  base=ჰ -->
		<map iso="B03" to="ჸ"/> <!-- C  base=ყ -->
		<map iso="B04" to="ჷ"/> <!-- V  base=ღ -->
		<map iso="B09" to="ჶ"/> <!-- .  base=ფ -->
		<map iso="B10" to="჻"/> <!-- /  base=წ -->
	</keyMap>
	<keyMap modifiers="altR+shift+caps? ctrl+alt+shift+caps?">
		<map iso="E00" to="~"/> <!-- `  base=„ -->
		<map iso="E03" to="§"/> <!-- 3  base=, -->
		<map iso="E08" to="°"/> <!-- 8  base=. -->
		<map iso="E09" to="«"/> <!-- 9  base=( -->
		<map iso="E10" to="»"/> <!-- 0  base=) -->
		<map iso="E11" to="_"/> <!-- - -->
		<map iso="D08" to="I"/> <!--   base=ნ -->
		<map iso="D11" to="{"/> <!-- [  base=კ -->
		<map iso="D12" to="}"/> <!-- ]  base=ქ -->
		<map iso="C03" to="D"/> <!--   base=ა -->
		<map iso="C09" to="L"/> <!--   base=რ -->
		<map iso="C11" to="\u{22}"/> <!-- '  base=გ  to= "  -->
		<map iso="C12" to="|"/> <!-- (key to right of ')  base=/ -->
		<map iso="B02" to="X"/> <!--   base=ჰ -->
		<map iso="B03" to="C"/> <!--   base=ყ -->
		<map iso="B04" to="V"/> <!--   base=ღ -->
		<map iso="B07" to="M"/> <!--   base=ც -->
		<map iso="B08" to="&lt;"/> <!-- ,  base=ლ -->
		<map iso="B09" to="&gt;"/> <!-- .  base=ფ -->
	</keyMap>
	<keyMap modifiers="ctrl+caps?">
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
</keyboard>
