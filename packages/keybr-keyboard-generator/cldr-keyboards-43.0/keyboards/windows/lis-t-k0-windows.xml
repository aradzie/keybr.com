<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="lis-t-k0-windows">
	<version platform="10" number="$Revision$"/>
	<names>
		<name value="Lisu (Basic)"/>
	</names>
	<settings fallback="omit"/>
	<keyMap>
		<map iso="E00" to="`"/>
		<map iso="E01" to="1"/>
		<map iso="E02" to="2"/>
		<map iso="E03" to="3"/>
		<map iso="E04" to="4"/>
		<map iso="E05" to="5"/>
		<map iso="E06" to="6"/>
		<map iso="E07" to="7"/>
		<map iso="E08" to="8"/>
		<map iso="E09" to="9"/>
		<map iso="E10" to="0"/>
		<map iso="E11" to="‐"/> <!-- - -->
		<map iso="E12" to="꓿"/> <!-- = -->
		<map iso="D01" to="Q"/>
		<map iso="D02" to="ꓪ"/> <!-- W -->
		<map iso="D03" to="ꓰ"/> <!-- E -->
		<map iso="D04" to="ꓣ"/> <!-- R -->
		<map iso="D05" to="ꓔ"/> <!-- T -->
		<map iso="D06" to="ꓬ"/> <!-- Y -->
		<map iso="D07" to="ꓴ"/> <!-- U -->
		<map iso="D08" to="ꓲ"/> <!-- I -->
		<map iso="D09" to="ꓳ"/> <!-- O -->
		<map iso="D10" to="ꓑ"/> <!-- P -->
		<map iso="D11" to="["/>
		<map iso="D12" to="]"/>
		<map iso="C01" to="ꓮ"/> <!-- A -->
		<map iso="C02" to="ꓢ"/> <!-- S -->
		<map iso="C03" to="ꓓ"/> <!-- D -->
		<map iso="C04" to="ꓝ"/> <!-- F -->
		<map iso="C05" to="ꓖ"/> <!-- G -->
		<map iso="C06" to="ꓧ"/> <!-- H -->
		<map iso="C07" to="ꓙ"/> <!-- J -->
		<map iso="C08" to="ꓗ"/> <!-- K -->
		<map iso="C09" to="ꓡ"/> <!-- L -->
		<map iso="C10" to="ꓼ"/> <!-- ; -->
		<map iso="C11" to="ʼ"/> <!-- ' -->
		<map iso="C12" to="\"/> <!-- (key to right of ') -->
		<map iso="B00" to="\"/> <!-- (key to left of Z) -->
		<map iso="B01" to="ꓜ"/> <!-- Z -->
		<map iso="B02" to="ꓫ"/> <!-- X -->
		<map iso="B03" to="ꓚ"/> <!-- C -->
		<map iso="B04" to="ꓦ"/> <!-- V -->
		<map iso="B05" to="ꓐ"/> <!-- B -->
		<map iso="B06" to="ꓠ"/> <!-- N -->
		<map iso="B07" to="ꓟ"/> <!-- M -->
		<map iso="B08" to=","/>
		<map iso="B09" to="."/>
		<map iso="B10" to="/"/>
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="shift+caps?">
		<map iso="E00" to="~"/> <!-- ` -->
		<map iso="E01" to="!"/> <!-- 1 -->
		<map iso="E02" to="@"/> <!-- 2 -->
		<map iso="E03" to="#"/> <!-- 3 -->
		<map iso="E04" to="$"/> <!-- 4 -->
		<map iso="E05" to="%"/> <!-- 5 -->
		<map iso="E06" to="^"/> <!-- 6 -->
		<map iso="E07" to="&amp;"/> <!-- 7 -->
		<map iso="E08" to="*"/> <!-- 8 -->
		<map iso="E09" to="("/> <!-- 9 -->
		<map iso="E10" to=")"/> <!-- 0 -->
		<map iso="E11" to="_"/> <!-- -  base=‐ -->
		<map iso="E12" to="+"/> <!-- =  base=꓿ -->
		<map iso="D02" to="ꓹꓼ"/> <!-- W  base=ꓪ -->
		<map iso="D03" to="ꓱ"/> <!-- E  base=ꓰ -->
		<map iso="D04" to="ꓤ"/> <!-- R  base=ꓣ -->
		<map iso="D05" to="ꓕ"/> <!-- T  base=ꓔ -->
		<map iso="D06" to="ꓻ"/> <!-- Y  base=ꓬ -->
		<map iso="D07" to="ꓵ"/> <!-- U  base=ꓴ -->
		<map iso="D08" to="꓾"/> <!-- I  base=ꓲ -->
		<map iso="D09" to="ˍ"/> <!-- O  base=ꓳ -->
		<map iso="D10" to="ꓒ"/> <!-- P  base=ꓑ -->
		<map iso="D11" to="{"/> <!-- [ -->
		<map iso="D12" to="}"/> <!-- ] -->
		<map iso="C01" to="ꓯ"/> <!-- A  base=ꓮ -->
		<map iso="C02" to="ꓸꓼ"/> <!-- S  base=ꓢ -->
		<map iso="C03" to="ꓷ"/> <!-- D  base=ꓓ -->
		<map iso="C04" to="ꓞ"/> <!-- F  base=ꓝ -->
		<map iso="C05" to="ꓨ"/> <!-- G  base=ꓖ -->
		<map iso="C06" to="ꓺ"/> <!-- H  base=ꓧ -->
		<map iso="C07" to="ꓩ"/> <!-- J  base=ꓙ -->
		<map iso="C08" to="ꓘ"/> <!-- K  base=ꓗ -->
		<map iso="C09" to="ꓶ"/> <!-- L  base=ꓡ -->
		<map iso="C10" to="ꓽ"/> <!-- ;  base=ꓼ -->
		<map iso="C11" to="\u{22}"/> <!-- '  base=ʼ  to= "  -->
		<map iso="C12" to="|"/> <!-- (key to right of ')  base=\ -->
		<map iso="B00" to="|"/> <!-- (key to left of Z)  base=\ -->
		<map iso="B03" to="ꓛ"/> <!-- C  base=ꓚ -->
		<map iso="B04" to="ꓥ"/> <!-- V  base=ꓦ -->
		<map iso="B05" to="ꓭ"/> <!-- B  base=ꓐ -->
		<map iso="B08" to="&lt;"/> <!-- , -->
		<map iso="B09" to="&gt;"/> <!-- . -->
		<map iso="B10" to="?"/> <!-- / -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
</keyboard>
