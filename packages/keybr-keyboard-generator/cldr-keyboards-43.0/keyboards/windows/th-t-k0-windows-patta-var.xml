<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="th-t-k0-windows-patta-var">
	<version platform="10" number="$Revision$"/>
	<names>
		<name value="Thai Pattachote (non-ShiftLock)"/>
	</names>
	<settings fallback="omit"/>
	<keyMap>
		<map iso="E00" to="_"/> <!-- ` -->
		<map iso="E01" to="="/> <!-- 1 -->
		<map iso="E02" to="๒"/> <!-- 2 -->
		<map iso="E03" to="๓"/> <!-- 3 -->
		<map iso="E04" to="๔"/> <!-- 4 -->
		<map iso="E05" to="๕"/> <!-- 5 -->
		<map iso="E06" to="\u{E39}"/> <!-- 6  to= ู  -->
		<map iso="E07" to="๗"/> <!-- 7 -->
		<map iso="E08" to="๘"/> <!-- 8 -->
		<map iso="E09" to="๙"/> <!-- 9 -->
		<map iso="E10" to="๐"/> <!-- 0 -->
		<map iso="E11" to="๑"/> <!-- - -->
		<map iso="E12" to="๖"/> <!-- = -->
		<map iso="D01" to="\u{E47}"/> <!-- Q  to= ็  -->
		<map iso="D02" to="ต"/> <!-- W -->
		<map iso="D03" to="ย"/> <!-- E -->
		<map iso="D04" to="อ"/> <!-- R -->
		<map iso="D05" to="ร"/> <!-- T -->
		<map iso="D06" to="\u{E48}"/> <!-- Y  to= ่  -->
		<map iso="D07" to="ด"/> <!-- U -->
		<map iso="D08" to="ม"/> <!-- I -->
		<map iso="D09" to="ว"/> <!-- O -->
		<map iso="D10" to="แ"/> <!-- P -->
		<map iso="D11" to="ใ"/> <!-- [ -->
		<map iso="D12" to="ฌ"/> <!-- ] -->
		<map iso="C01" to="\u{E49}"/> <!-- A  to= ้  -->
		<map iso="C02" to="ท"/> <!-- S -->
		<map iso="C03" to="ง"/> <!-- D -->
		<map iso="C04" to="ก"/> <!-- F -->
		<map iso="C05" to="\u{E31}"/> <!-- G  to= ั  -->
		<map iso="C06" to="\u{E35}"/> <!-- H  to= ี  -->
		<map iso="C07" to="า"/> <!-- J -->
		<map iso="C08" to="น"/> <!-- K -->
		<map iso="C09" to="เ"/> <!-- L -->
		<map iso="C10" to="ไ"/> <!-- ; -->
		<map iso="C11" to="ข"/> <!-- ' -->
		<map iso="C12" to="\u{F8C7}"/> <!-- (key to right of ') -->
		<map iso="B00" to="\u{F8C7}"/> <!-- (key to left of Z) -->
		<map iso="B01" to="บ"/> <!-- Z -->
		<map iso="B02" to="ป"/> <!-- X -->
		<map iso="B03" to="ล"/> <!-- C -->
		<map iso="B04" to="ห"/> <!-- V -->
		<map iso="B05" to="\u{E34}"/> <!-- B  to= ิ  -->
		<map iso="B06" to="ค"/> <!-- N -->
		<map iso="B07" to="ส"/> <!-- M -->
		<map iso="B08" to="ะ"/> <!-- , -->
		<map iso="B09" to="จ"/> <!-- . -->
		<map iso="B10" to="พ"/> <!-- / -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="shift+caps?">
		<map iso="E00" to="฿"/> <!-- `  base=_ -->
		<map iso="E01" to="+"/> <!-- 1  base== -->
		<map iso="E02" to="\u{22}"/> <!-- 2  base=๒  to= "  -->
		<map iso="E03" to="/"/> <!-- 3  base=๓ -->
		<map iso="E04" to=","/> <!-- 4  base=๔ -->
		<map iso="E05" to="?"/> <!-- 5  base=๕ -->
		<map iso="E06" to="\u{E38}"/> <!-- 6  base=ู  to= ุ  -->
		<map iso="E07" to="_"/> <!-- 7  base=๗ -->
		<map iso="E08" to="."/> <!-- 8  base=๘ -->
		<map iso="E09" to="("/> <!-- 9  base=๙ -->
		<map iso="E10" to=")"/> <!-- 0  base=๐ -->
		<map iso="E11" to="-"/> <!--   base=๑ -->
		<map iso="E12" to="%"/> <!-- =  base=๖ -->
		<map iso="D01" to="\u{E4A}"/> <!-- Q  base=็  to= ๊  -->
		<map iso="D02" to="ฤ"/> <!-- W  base=ต -->
		<map iso="D03" to="ๆ"/> <!-- E  base=ย -->
		<map iso="D04" to="ญ"/> <!-- R  base=อ -->
		<map iso="D05" to="ษ"/> <!-- T  base=ร -->
		<map iso="D06" to="\u{E36}"/> <!-- Y  base=่  to= ึ  -->
		<map iso="D07" to="ฝ"/> <!-- U  base=ด -->
		<map iso="D08" to="ซ"/> <!-- I  base=ม -->
		<map iso="D09" to="ถ"/> <!-- O  base=ว -->
		<map iso="D10" to="ฒ"/> <!-- P  base=แ -->
		<map iso="D11" to="ฯ"/> <!-- [  base=ใ -->
		<map iso="D12" to="ฦ"/> <!-- ]  base=ฌ -->
		<map iso="C01" to="\u{E4B}"/> <!-- A  base=้  to= ๋  -->
		<map iso="C02" to="ธ"/> <!-- S  base=ท -->
		<map iso="C03" to="ำ"/> <!-- D  base=ง -->
		<map iso="C04" to="ณ"/> <!-- F  base=ก -->
		<map iso="C05" to="\u{E4C}"/> <!-- G  base=ั  to= ์  -->
		<map iso="C06" to="\u{E37}"/> <!-- H  base=ี  to= ื  -->
		<map iso="C07" to="ผ"/> <!-- J  base=า -->
		<map iso="C08" to="ช"/> <!-- K  base=น -->
		<map iso="C09" to="โ"/> <!-- L  base=เ -->
		<map iso="C10" to="ฆ"/> <!-- ;  base=ไ -->
		<map iso="C11" to="ฑ"/> <!-- '  base=ข -->
		<map iso="C12" to="\u{E4D}"/> <!-- (key to right of ')  base=  to= ํ  -->
		<map iso="B00" to="\u{E4D}"/> <!-- (key to left of Z)  base=  to= ํ  -->
		<map iso="B01" to="ฎ"/> <!-- Z  base=บ -->
		<map iso="B02" to="ฏ"/> <!-- X  base=ป -->
		<map iso="B03" to="ฐ"/> <!-- C  base=ล -->
		<map iso="B04" to="ภ"/> <!-- V  base=ห -->
		<map iso="B05" to="\u{E31}"/> <!-- B  base=ิ  to= ั  -->
		<map iso="B06" to="ศ"/> <!-- N  base=ค -->
		<map iso="B07" to="ฮ"/> <!-- M  base=ส -->
		<map iso="B08" to="ฟ"/> <!-- ,  base=ะ -->
		<map iso="B09" to="ฉ"/> <!-- .  base=จ -->
		<map iso="B10" to="ฬ"/> <!-- /  base=พ -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="ctrl+caps?">
		<map iso="D11" to="\u{1B}"/> <!-- [  base=ใ -->
		<map iso="D12" to="\u{1D}"/> <!-- ]  base=ฌ -->
		<map iso="C12" to="\u{1C}"/> <!-- (key to right of ')  base= -->
		<map iso="B00" to="\u{1C}"/> <!-- (key to left of Z)  base= -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
</keyboard>
