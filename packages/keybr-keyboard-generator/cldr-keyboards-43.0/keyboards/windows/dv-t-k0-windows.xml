<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="dv-t-k0-windows">
	<version platform="10" number="$Revision$"/>
	<names>
		<name value="Divehi Phonetic"/>
	</names>
	<settings fallback="omit"/>
	<keyMap>
		<map iso="E00" to="`"/>
		<map iso="E01" to="1"/>
		<map iso="E02" to="2"/>
		<map iso="E03" to="3"/>
		<map iso="E04" to="4"/>
		<map iso="E05" to="5"/>
		<map iso="E06" to="6"/>
		<map iso="E07" to="7"/>
		<map iso="E08" to="8"/>
		<map iso="E09" to="9"/>
		<map iso="E10" to="0"/>
		<map iso="E11" to="-"/>
		<map iso="E12" to="="/>
		<map iso="D01" to="\u{7B0}"/> <!-- Q  to= ް  -->
		<map iso="D02" to="އ"/> <!-- W -->
		<map iso="D03" to="\u{7AC}"/> <!-- E  to= ެ  -->
		<map iso="D04" to="ރ"/> <!-- R -->
		<map iso="D05" to="ތ"/> <!-- T -->
		<map iso="D06" to="ޔ"/> <!-- Y -->
		<map iso="D07" to="\u{7AA}"/> <!-- U  to= ު  -->
		<map iso="D08" to="\u{7A8}"/> <!-- I  to= ި  -->
		<map iso="D09" to="\u{7AE}"/> <!-- O  to= ޮ  -->
		<map iso="D10" to="ޕ"/> <!-- P -->
		<map iso="D11" to="]"/> <!-- [ -->
		<map iso="D12" to="["/> <!-- ] -->
		<map iso="C01" to="\u{7A6}"/> <!-- A  to= ަ  -->
		<map iso="C02" to="ސ"/> <!-- S -->
		<map iso="C03" to="ދ"/> <!-- D -->
		<map iso="C04" to="ފ"/> <!-- F -->
		<map iso="C05" to="ގ"/> <!-- G -->
		<map iso="C06" to="ހ"/> <!-- H -->
		<map iso="C07" to="ޖ"/> <!-- J -->
		<map iso="C08" to="ކ"/> <!-- K -->
		<map iso="C09" to="ލ"/> <!-- L -->
		<map iso="C10" to="؛"/> <!-- ; -->
		<map iso="C11" to="&apos;"/>
		<map iso="C12" to="\"/> <!-- (key to right of ') -->
		<map iso="B00" to="\"/> <!-- (key to left of Z) -->
		<map iso="B01" to="ޒ"/> <!-- Z -->
		<map iso="B02" to="×"/> <!-- X -->
		<map iso="B03" to="ޗ"/> <!-- C -->
		<map iso="B04" to="ވ"/> <!-- V -->
		<map iso="B05" to="ބ"/> <!-- B -->
		<map iso="B06" to="ނ"/> <!-- N -->
		<map iso="B07" to="މ"/> <!-- M -->
		<map iso="B08" to="،"/> <!-- , -->
		<map iso="B09" to="."/>
		<map iso="B10" to="/"/>
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="shift+caps?">
		<map iso="E00" to="~"/> <!-- ` -->
		<map iso="E01" to="!"/> <!-- 1 -->
		<map iso="E02" to="@"/> <!-- 2 -->
		<map iso="E03" to="#"/> <!-- 3 -->
		<map iso="E04" to="$"/> <!-- 4 -->
		<map iso="E05" to="%"/> <!-- 5 -->
		<map iso="E06" to="^"/> <!-- 6 -->
		<map iso="E07" to="&amp;"/> <!-- 7 -->
		<map iso="E08" to="*"/> <!-- 8 -->
		<map iso="E09" to=")"/> <!-- 9 -->
		<map iso="E10" to="("/> <!-- 0 -->
		<map iso="E11" to="_"/> <!-- - -->
		<map iso="E12" to="+"/> <!-- = -->
		<map iso="D01" to="ޤ"/> <!-- Q  base=ް -->
		<map iso="D02" to="ޢ"/> <!-- W  base=އ -->
		<map iso="D03" to="\u{7AD}"/> <!-- E  base=ެ  to= ޭ  -->
		<map iso="D04" to="ޜ"/> <!-- R  base=ރ -->
		<map iso="D05" to="ޓ"/> <!-- T  base=ތ -->
		<map iso="D06" to="ޠ"/> <!-- Y  base=ޔ -->
		<map iso="D07" to="\u{7AB}"/> <!-- U  base=ު  to= ޫ  -->
		<map iso="D08" to="\u{7A9}"/> <!-- I  base=ި  to= ީ  -->
		<map iso="D09" to="\u{7AF}"/> <!-- O  base=ޮ  to= ޯ  -->
		<map iso="D10" to="÷"/> <!-- P  base=ޕ -->
		<map iso="D11" to="}"/> <!-- [  base=] -->
		<map iso="D12" to="{"/> <!-- ]  base=[ -->
		<map iso="C01" to="\u{7A7}"/> <!-- A  base=ަ  to= ާ  -->
		<map iso="C02" to="ށ"/> <!-- S  base=ސ -->
		<map iso="C03" to="ޑ"/> <!-- D  base=ދ -->
		<map iso="C04" to="ﷲ"/> <!-- F  base=ފ -->
		<map iso="C05" to="ޣ"/> <!-- G  base=ގ -->
		<map iso="C06" to="ޙ"/> <!-- H  base=ހ -->
		<map iso="C07" to="ޛ"/> <!-- J  base=ޖ -->
		<map iso="C08" to="ޚ"/> <!-- K  base=ކ -->
		<map iso="C09" to="ޅ"/> <!-- L  base=ލ -->
		<map iso="C10" to=":"/> <!-- ;  base=؛ -->
		<map iso="C11" to="\u{22}"/> <!-- '  to= "  -->
		<map iso="C12" to="|"/> <!-- (key to right of ')  base=\ -->
		<map iso="B00" to="|"/> <!-- (key to left of Z)  base=\ -->
		<map iso="B01" to="ޡ"/> <!-- Z  base=ޒ -->
		<map iso="B02" to="ޘ"/> <!-- X  base=× -->
		<map iso="B03" to="ޝ"/> <!-- C  base=ޗ -->
		<map iso="B04" to="ޥ"/> <!-- V  base=ވ -->
		<map iso="B05" to="ޞ"/> <!-- B  base=ބ -->
		<map iso="B06" to="ޏ"/> <!-- N  base=ނ -->
		<map iso="B07" to="ޟ"/> <!-- M  base=މ -->
		<map iso="B08" to="&gt;"/> <!-- ,  base=، -->
		<map iso="B09" to="&lt;"/> <!-- . -->
		<map iso="B10" to="؟"/> <!-- / -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="altR+caps? ctrl+alt+caps?">
		<map iso="C10" to=";"/> <!--   base=؛ -->
		<map iso="B04" to="\u{200D}"/> <!-- V  base=ވ -->
		<map iso="B05" to="\u{200C}"/> <!-- B  base=ބ -->
		<map iso="B06" to="\u{200E}"/> <!-- N  base=ނ -->
		<map iso="B07" to="\u{200F}"/> <!-- M  base=މ -->
		<map iso="B08" to=","/> <!--   base=، -->
	</keyMap>
	<keyMap modifiers="ctrl+caps?">
		<map iso="D11" to="\u{1B}"/> <!-- [  base=] -->
		<map iso="D12" to="\u{1D}"/> <!-- ]  base=[ -->
		<map iso="C12" to="\u{1C}"/> <!-- (key to right of ')  base=\ -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
</keyboard>
