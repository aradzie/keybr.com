		<map iso="D07" to="\u{316}"/> <!-- U  to= ̖  -->
		<map iso="D08" to="\u{309}"/> <!-- I  to= ̉  -->
		<map iso="D09" to="\u{30F}"/> <!-- O  to= ̏  -->
		<map iso="D10" to="\u{310}"/> <!-- P  to= ̐  -->
		<map iso="C01" to="\u{300}"/> <!-- A  to= ̀  -->
