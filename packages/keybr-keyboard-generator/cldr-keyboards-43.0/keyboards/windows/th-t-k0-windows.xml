		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="ctrl+caps?">
		<map iso="D11" to="%"/> <!-- [  base=บ -->
		<map iso="D12" to="๑"/> <!-- ]  base=ล -->
		<map iso="C12" to="+"/> <!-- (key to right of ')  base=ฃ -->
		<map iso="B00" to="๒"/> <!-- (key to left of Z)  base=ฃ -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
</keyboard>
