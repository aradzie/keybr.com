		<map iso="C03" to="ก"/> <!-- D -->
		<map iso="C04" to="ด"/> <!-- F -->
		<map iso="C05" to="เ"/> <!-- G -->
		<map iso="C06" to="\u{E49}"/> <!-- H  to= ้  -->
		<map iso="C07" to="\u{E48}"/> <!-- J  to= ่  -->
		<map iso="C08" to="า"/> <!-- K -->
		<map iso="C09" to="ส"/> <!-- L -->
		<map iso="C10" to="ว"/> <!-- ; -->
		<map iso="C11" to="ง"/> <!-- ' -->
		<map iso="C12" to="ฃ"/> <!-- (key to right of ') -->
		<map iso="B00" to="ฃ"/> <!-- (key to left of Z) -->
