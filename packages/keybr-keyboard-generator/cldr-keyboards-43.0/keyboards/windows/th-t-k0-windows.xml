		<map iso="C02" to="ฆ"/> <!-- S  base=ห -->
		<map iso="C03" to="ฏ"/> <!-- D  base=ก -->
		<map iso="C04" to="โ"/> <!-- F  base=ด -->
		<map iso="C05" to="ฌ"/> <!-- G  base=เ -->
		<map iso="C06" to="\u{E47}"/> <!-- H  base=้  to= ็  -->
		<map iso="C07" to="\u{E4B}"/> <!-- J  base=่  to= ๋  -->
		<map iso="C08" to="ษ"/> <!-- K  base=า -->
		<map iso="C09" to="ศ"/> <!-- L  base=ส -->
		<map iso="C10" to="ซ"/> <!-- ;  base=ว -->
		<map iso="C11" to="."/> <!-- '  base=ง -->
		<map iso="C12" to="ฅ"/> <!-- (key to right of ')  base=ฃ -->
		<map iso="B00" to="ฅ"/> <!-- (key to left of Z)  base=ฃ -->
		<map iso="B01" to="("/> <!-- Z  base=ผ -->
		<map iso="B02" to=")"/> <!-- X  base=ป -->
		<map iso="B03" to="ฉ"/> <!-- C  base=แ -->
		<map iso="B04" to="ฮ"/> <!-- V  base=อ -->
		<map iso="B05" to="\u{E3A}"/> <!-- B  base=ิ  to= ฺ  -->
		<map iso="B06" to="\u{E4C}"/> <!-- N  base=ื  to= ์  -->
		<map iso="B07" to="?"/> <!-- M  base=ท -->
		<map iso="B08" to="ฒ"/> <!-- ,  base=ม -->
		<map iso="B09" to="ฬ"/> <!-- .  base=ใ -->
