		<map iso="B10" to="ฝ"/> <!-- / -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="shift caps">
		<map iso="E00" to="%"/> <!-- `  base=_ -->
		<map iso="E01" to="+"/> <!-- 1  base=ๅ -->
		<map iso="E02" to="๑"/> <!-- 2  base=/ -->
