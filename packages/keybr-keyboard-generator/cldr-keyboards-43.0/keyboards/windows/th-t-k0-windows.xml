		<name value="Thai Kedmanee"/>
	</names>
	<settings fallback="omit"/>
	<keyMap>
		<map iso="E00" to="_"/> <!-- ` -->
		<map iso="E01" to="ๅ"/> <!-- 1 -->
		<map iso="E02" to="/"/> <!-- 2 -->
		<map iso="E03" to="-"/> <!-- 3 -->
		<map iso="E04" to="ภ"/> <!-- 4 -->
		<map iso="E05" to="ถ"/> <!-- 5 -->
